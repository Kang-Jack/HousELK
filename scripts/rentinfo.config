  }
}
filter {
    mutate {convert => ["price", "integer"]} 
    date {
        match => ["time", "ISO8601"]
    }
}
output {
    if [type] == "rentinfo" {
        elasticsearch {
            hosts => ["localhost:9200"]
            action => "index"
            index => "%{type}" 
            document_id => "%{houseid}"
        }
    }
}